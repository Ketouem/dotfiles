[core]
    editor = vim
[color]
    ui = true
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    type = cat-file -t
    dump = cat-file -p
    pullr = pull --rebase
    unadd = reset HEAD
[user]
    name = Ketouem
    email = ketouem@gmail.com
[push]
    default = current
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[merge]
    ff = only
    commit = no

